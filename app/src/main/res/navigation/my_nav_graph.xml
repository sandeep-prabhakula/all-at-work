<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_nav_graph"
    app:startDestination="@id/loginFragment">

    <fragment
        android:id="@+id/loginFragment"
        android:name="com.sandeepprabhakula.smartindiahackathon.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_registerForServiceProvider"
            app:destination="@id/registerForServiceProvider" />
    </fragment>
    <fragment
        android:id="@+id/registerForServiceProvider"
        android:name="com.sandeepprabhakula.smartindiahackathon.RegisterForServiceProvider"
        android:label="fragment_register_for_service_provider"
        tools:layout="@layout/fragment_register_for_service_provider" />
</navigation>